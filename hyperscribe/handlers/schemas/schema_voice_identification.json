{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "speaker": {
        "type": "string",
        "minLength": 1
      },
      "voice": {
        "type": "string",
        "pattern": "^voice_[1-9]\\d*$"
      }
    },
    "required": [
      "speaker",
      "voice"
    ],
    "additionalProperties": false
  },
  "minItems": 1,
  "uniqueItems": true
}
