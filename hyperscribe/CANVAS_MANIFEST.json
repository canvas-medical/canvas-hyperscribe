{
  "sdk_version": "0.28.0",
  "plugin_version": "0.0.61",
  "name": "hyperscribe",
  "description": "Create commands based on the content of an audio discussion between a patient and a provider.",
  "url_permissions": [
    {
      "url": "https://testing-playground-hyperscribe-audio-server.replit.app/",
      "permissions": [
        "ALLOW_SAME_ORIGIN",
        "MICROPHONE",
        "SCRIPTS"
      ]
    },
    {
      "url": "https://encounter-copilot-audio-server-andrewhines8.replit.app/",
      "permissions": [
        "ALLOW_SAME_ORIGIN",
        "MICROPHONE",
        "SCRIPTS"
      ]
    },
    {
      "url": "https://*.s3.*.amazonaws.com/*",
      "permissions": [
        "ALLOW_SAME_ORIGIN"
      ]
    }
  ],
  "components": {
    "protocols": [
      {
        "class": "hyperscribe.handlers.commander:Commander",
        "description": "Commander add commands based on vocal instructions",
        "data_access": {
          "event": "",
          "read": [],
          "write": []
        }
      },
      {
        "class": "hyperscribe.handlers.launcher:Launcher",
        "description": "Action button on the note to launch Hyperscribe with note context",
        "data_access": {
          "event": "",
          "read": [],
          "write": []
        }
      }
    ],
    "applications": [
      {
        "class": "hyperscribe.handlers.progress.progress:Progress",
        "name": "Hyperscribe Progress Displayer",
        "description": "Display the known log of the Hyperscribe",
        "icon": "handlers/progress/progress.png",
        "scope": "patient_specific"
      }
    ],
    "commands": [],
    "content": [],
    "effects": [],
    "views": []
  },
  "secrets": [
    "VendorTextLLM",
    "KeyTextLLM",
    "VendorAudioLLM",
    "KeyAudioLLM",
    "ScienceHost",
    "OntologiesHost",
    "PreSharedKey",
    "AudioHost",
    "AudioIntervalSeconds",
    "StructuredReasonForVisit",
    "AuditLLMDecisions",
    "AwsKey",
    "AwsSecret",
    "AwsRegion",
    "AwsBucket"
  ],
  "tags": {},
  "references": [],
  "license": "",
  "diagram": false,
  "readme": "./README.md"
